<div class="container col-8">
  <div class="d-flex align-items-center justify-content-between">
    <h1>Welcome to Find Venue</h1>
  </div>


  <div class="container">
    <p1>Website created by Ronan, Jan and Jaro that will allow you to rent a venue.</p1>
      <% @venues.last(6).each do |venue| %>
      <div>
        <%= venue.name %>
      </div>
      <div>
        <%= link_to "Find out more...", venue_path(venue), class: "btn btn-secondary"%>
      </div>
      <br>
      <% end %>
  </div>
</div>
